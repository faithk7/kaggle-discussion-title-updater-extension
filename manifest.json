{
    "manifest_version": 3,
    "name": "Test Extension",
    "version": "1.0",
    "description": "A test Chrome extension using Manifest V3 - haha.",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {},
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
